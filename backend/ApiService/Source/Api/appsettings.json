{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "Microsoft.AspNetCore": "Warning",
                "System": "Error"
            }
        },
        "Enrich": [
            "FromLogContext",
            "WithEnvironmentName",
            "WithMachineName"
        ]
    },
    "Logging": {
        "LogLevel": {
            "Default": "None"
        }
    },
    "ConnectionStrings": {
        "DbConnectionString": "Host=localhost;Port=5432;Database=mydb;Username=admin;Password=secret"
    },
    "AllowedHosts": "*"
}
